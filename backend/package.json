{
  "name": "ipauth-backend",
  "version": "1.0.0",
  "description": "IP-based License Authentication Platform Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node -r ts-node/register src/scripts/migrate.ts",
    "seed": "node -r ts-node/register src/scripts/seed.ts",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
